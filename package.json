{
  "name": "Rentzila",
  "type": "module",
  "scripts": {
    "wdio-all-chrome": "npx wdio run ./wdio.conf.ts",
    "wdio-all-chrome-headless": "npx wdio run ./wdio.conf.headless.ts",
    "wdio-footer-only-chrome": "npx wdio run ./wdio.conf.ts --spec ./test/specs/mainPage/footer.ts",
    "wdio-C214-chrome": "npx wdio run ./wdio.conf.ts --mochaOpts.grep C214",
    "wdio-authorization-chrome": "npx wdio run ./wdio.conf.ts --suite authorization",
    "allure-generate": "allure generate --clean && allure open"
  },
  "dependencies": {
    "@wdio/allure-reporter": "*",
    "@wdio/cli": "*",
    "@wdio/local-runner": "*",
    "@wdio/mocha-framework": "*",
    "@wdio/spec-reporter": "*",
    "axios": "*",
    "dotenv": "*"
  }
}
